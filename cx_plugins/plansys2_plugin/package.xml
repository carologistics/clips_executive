<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cx_plansys2_plugin</name>
  <version>0.0.0</version>
  <description>Consists of all plugins that provide functionality to CLIPS and an implemented manager unit</description>

  <maintainer email="viehmann@kbsg.rwth-aachen.de">Tarik Viehmann</maintainer>

  <license>GPLv2+ license</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>cx_plugin</depend>
  <depend>cx_utils</depend>
  <depend>pluginlib</depend>
  <depend>plansys2_problem_expert</depend>
  <depend>plansys2_domain_expert</depend>
  <depend>plansys2_planner</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>cx_clips_executive</test_depend>
  <test_depend>ros2run</test_depend>


  <export>
    <build_type>ament_cmake</build_type>
    <cx_plugin plugin="${prefix}/plansys2_plugin.xml"/>
  </export>
</package>
