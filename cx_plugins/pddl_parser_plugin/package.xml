<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cx_pddl_parser_plugin</name>
  <version>0.1.0</version>
  <description>Provide pddl parsing functions and deftemplates to Clips.</description>

  <maintainer email="viehmann@kbsg.rwth-aachen.de">Tarik Viehmann</maintainer>

  <license>GPLv2+ license</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>cx_plugin</depend>
  <depend>cx_utils</depend>
  <depend>pddl_parser</depend>
  <depend>pluginlib</depend>
  <depend>clips_vendor</depend>


  <export>
    <build_type>ament_cmake</build_type>
    <cx_plugin plugin="${prefix}/pddl_parser_plugin.xml"/>
  </export>
</package>
